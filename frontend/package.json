    "wretch": "^1.7.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.5",
